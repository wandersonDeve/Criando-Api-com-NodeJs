{
  "compilerOptions": {
    
    "target": "es2016",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@modules/*":["modules/*"],
      "@config/*":["config/*"],
      "@shared/*":["shared/*"],
      "@err/*":["err/*"],
      "@utils/*":["utils/*"]
    }, 
    "typeRoots": [
      "src/@types",
      "node_modules/@types"
    ] ,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
